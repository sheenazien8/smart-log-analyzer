{
    "name": "sheenazien8/smart-log-analyzer",
    "description": "AI-powered Laravel package for analyzing application logs, detecting patterns, and providing actionable insights",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "logs",
        "analysis",
        "ai",
        "monitoring",
        "error-tracking",
        "anomaly-detection"
    ],
    "authors": [
        {
            "name": "Smart Log Analyzer Team",
            "email": "support@smartloganalyzer.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/mail": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "SmartLogAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartLogAnalyzer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartLogAnalyzer\\SmartLogAnalyzerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/phpcs --standard=PSR12 src/",
        "fix": "vendor/bin/phpcbf --standard=PSR12 src/",
        "analyse": "vendor/bin/phpstan analyse src/ --level=8"
    }
}
